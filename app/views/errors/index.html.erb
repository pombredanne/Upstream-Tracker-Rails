<h1>Listing records</h1>

<table>
  <tr>
    <th>Package Name</th>
    <th>Branch</th>
    <th>Method</th>
    <th>URL</th>
    <th></th>
  </tr>

<% @errors.each do |record| %>
  <tr>
    <td><%= record.pkgName %></td>
    <td><%= record.branch %></td>
    <td><%= record.method %></td>
    <td><%= record.info %></td>
    <td><%= link_to 'Edit', edit_record_path(record) %></td>
  </tr>
<% end %>
</table>